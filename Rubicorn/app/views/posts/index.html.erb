<h1>All SalesCall Feedback</h1>

<table>
  <tr>
    <th>Post Title</th>
    <th>Post Text</th>
  </tr>

  <% @posts.each do |post| %>
      <tr>
        <td><%= post.title %></td>
        <td><%= post.text %></td>
      </tr>
  <% end %>

  <%= image_tag("rubicorn.png") %>

</table>

<%= link_to 'Create a new post', new_post_path %>